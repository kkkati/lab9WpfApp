<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:col1="clr-namespace:System.Collections;assembly=mscorlib"
                    xmlns:sys1="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <col1:ArrayList x:Key="font">
        <sys1:String>Arial</sys1:String>
        <sys1:String>Times New Roman</sys1:String>
        <sys1:String>Segoe UI</sys1:String>
    </col1:ArrayList>
    <col1:ArrayList x:Key="size">
        <sys1:Double>12</sys1:Double>
        <sys1:Double>14</sys1:Double>
        <sys1:Double>16</sys1:Double>
    </col1:ArrayList>
</ResourceDictionary>